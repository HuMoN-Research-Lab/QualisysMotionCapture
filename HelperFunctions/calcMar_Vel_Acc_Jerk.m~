function [marVel,marAcc,marJerk,marJerk_squared] = calcMar_Vel_Acc_Jerk(segCenter,totalCOMXYZ);

%% Marker tracking hip data
%hipIDs were acquired using the markerLabels
marVel =  [];
marAcc =  [];
marJerk = [];

%Velocity of =
marVelx =           diff(totalCOMXYZ(1,:));
marVelz =           diff(totalCOMXYZ(3,:));
marVel =            plus(marVelx,marVelz);
marAcc =            diff(marVel);
marJerk =           diff(marAcc);
marJerk_squared =   (marJerk).^2;

%calculates acceleration of hips
%hipsAcc = [0 0 0; diff(hipsVel)];

end

